{
  "mcpServers": {
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref",
        "REDACTED_SUPABASE_PROJECT_REF",
        "--read-only"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "REDACTED_SUPABASE_ACCESS_TOKEN"
      }
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    }
  }
}